{
  "swagger": {
    "outputDirectory": "./dist",
    "entryFile": "./src/server.ts",
    "host": "localhost:3000",
    "basePath": "/v1",
    "securityDefinitions": {
      "jwt": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "routes": {
    "basePath": "/v1",
    "entryFile": "./src/server.ts",
    "routesDir": "./src",
    "iocModule": "./src/inversify/ioc",
    "authenticationModule": "./src/authentication.ts"
  }
}